namespace ca.mcgill.ecse223.tileo.model;

class ActionTile {
	InactivityStatus {
  	Active {
  		land / {
  			doLand();
  			setTurnsUntilActive(inactivityPeriod+1);
  		} -> Inactive;	
  	}
  	
  	Inactive {
  		inactivityPeriodCompleted -> Active;
  		land / {
  			doLandAsNormal();
  		} -> Inactive;
  	}
  }
}